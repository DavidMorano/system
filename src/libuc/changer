#!/usr/extra/bin/ksh
# CHANGER

NAMES=
NAMES="${NAMES} paramfile_enum "

DN=/dev/null

for F in "${@}" ; do
  if [[ -r "${F}" ]] ; then
    for N in ${NAMES} ; do
      if fgrep -q "${N}" $F > ${DN} ; then
      ed - $F <<\EOF
g/paramfile_enum/s//paramfile_curenum/g
	w
	q
EOF
      fi
    done
  fi
done


