#!/usr/extra/bin/ksh
# CHANGER

NAMES=
NAMES="${NAMES} vecobj_fetch "

DN=/dev/null

for F in "${@}" ; do
  if [[ -r "${F}" ]] ; then
    for N in ${NAMES} ; do
      if fgrep -q "${N}" $F > ${DN} ; then
      ed - $F <<\EOF
g/vecobj_fetch/s//vecobj_curfetch/g
	w
	q
EOF
      fi
    done
  fi
done


