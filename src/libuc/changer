#!/usr/extra/bin/ksh
# CHANGER

NAMES=
NAMES="${NAMES} filemap_getline"


DN=/dev/null

for F in "${@}" ; do
  if [[ -r "${F}" ]] ; then
    for N in ${NAMES} ; do
      if fgrep ${N} $F > ${DN} ; then
      ed - $F <<\EOF
g/filemap_getline/s//filemap_getln/g
	w
	q
EOF
      fi
    done
  fi
done


