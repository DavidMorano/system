#!/usr/extra/bin/ksh
# CHANGER

NAMES=
NAMES="${NAMES} paramopt_enumkeys"

DN=/dev/null

for F in "${@}" ; do
  if [[ -r "${F}" ]] ; then
    for N in ${NAMES} ; do
      if fgrep -q "${N}" $F > ${DN} ; then
      ed - $F <<\EOF
g/paramopt_enumkeys/s//paramopt_curenumkey/g
	w
	q
EOF
      fi
    done
  fi
done


