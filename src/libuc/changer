#!/usr/extra/bin/ksh
# CHANGER

NAMES=
NAMES="${NAMES} mkplogid mksublogid"

DN=/dev/null

for F in "${@}" ; do
  if [[ -r "${F}" ]] ; then
    for N in ${NAMES} ; do
      if fgrep -q "${N}" $F > ${DN} ; then
      ed - $F <<\EOF
g/mkplogid/s//mklogidpre/g
g/mksublogid/s//mklogidsub/g
	w
	q
EOF
      fi
    done
  fi
done


