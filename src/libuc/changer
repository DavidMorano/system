#!/usr/extra/bin/ksh
# CHANGER

NAMES=
NAMES="${NAMES} libmem.alloc"

DN=/dev/null

for F in "${@}" ; do
  if [[ -r "${F}" ]] ; then
    for N in ${NAMES} ; do
      if fgrep -q "${N}" $F > ${DN} ; then
      ed - $F <<\EOF
g/libmem.alloc/s//libmem.malloc/g
g/libmem.realloc/s//libmem.ralloc/g
	w
	q
EOF
      fi
    done
  fi
done


