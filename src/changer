#!/usr/extra/bin/ksh
# CHANGER

NAMES=
NAMES="${NAMES} dialer_free "

DN=/dev/null

for F in "${@}" ; do
  if [[ -r "${F}" ]] ; then
    for N in ${NAMES} ; do
      if fgrep -q "${N}" $F > ${DN} ; then
      ed - $F <<\EOF
g/sysdialer_free/s//sysdialer_finish/g
	w
	q
EOF
      fi
    done
  fi
done


