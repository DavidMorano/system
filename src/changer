#!/usr/extra/bin/ksh
# CHANGER

NAMES=
NAMES="${NAMES} ucmallreg_enum"

DN=/dev/null

for F in "${@}" ; do
  if [[ -r "${F}" ]] ; then
    for N in ${NAMES} ; do
      if fgrep "${N}" $F > ${DN} ; then
      ed - $F <<\EOF
g/ucmallreg_enum/s//ucmallreg_curenum/g
	w
	q
EOF
      fi
    done
  fi
done


